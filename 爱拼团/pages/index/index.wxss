}
.apt-index .apt-index-slideShow swiper swiper-item image{
  width: 100%;
  height: 100%;
}
.apt-index .apt-index-nav image {
  height: 150rpx;
  width: 150rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.apt-index-floor .floor_title {
